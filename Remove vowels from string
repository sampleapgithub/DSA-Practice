class Solution {
    String removeVowels(String s) {
        // code here
        String vowels = "aeiou";
        
        StringBuilder result = new StringBuilder();
        
        for(int i =0; i<s.length(); i++){
            
            char ch = s.charAt(i);
            if(vowels.indexOf(ch)== -1){
                result.append(ch);
            }
        }
        return result.toString();
    }
}
