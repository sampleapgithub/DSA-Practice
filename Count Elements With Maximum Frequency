class Solution {
    public int maxFrequencyElements(int[] nums) {
        HashMap<Integer, Integer> freq = new HashMap<>();
        for(int num : nums){
            freq.put(num, freq.getOrDefault(num, 0) +1);
        }

        int maxFreq=0;
        for(int value : freq.values()){
            if(value > maxFreq){
                maxFreq = value;
            }
        }
        int result = 0;
        for(int value : freq.values()){
            if (value == maxFreq){
                result += value;
            }
        }
        return result;
    }
}
